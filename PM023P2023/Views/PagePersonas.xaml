<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PM023P2023.Views.PagePersonas">
    <ContentPage.Content>
        <StackLayout>

            <Entry Keyboard="Text"  Placeholder="Indique sus nombres"  x:Name="txtnombre" />

            <Entry Keyboard="Text"  Placeholder="Indique sus apellidos"  x:Name="txtapellido" />

            <DatePicker x:Name="fechanac" Format="D" />

            <Entry Keyboard="Numeric"  Placeholder="Indique su telefono"  x:Name="txttelefono" />

            <Button x:Name="btnproceso" Text="Procesar" Clicked="btnproceso_Clicked"  />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>